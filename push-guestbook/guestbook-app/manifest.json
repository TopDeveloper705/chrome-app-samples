{
  "name": "Push Guestbook Sample",
  "version": "1.24",
  "manifest_version": 2,

  "description": "Displays the last post to an anonymous guestbook using push messaging",
  "icons": { "16": "icon_16.png", "128": "icon_128.png" },

  "app": {
    "background": {
      "scripts": ["Guestbook.js", "background.js"]
    }
  },

  "permissions": [
    "http://localhost:8080/monitor",
    "pushMessaging"
  ]
}
